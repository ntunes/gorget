from std.collections import Vector

void main():
    Vector[int] v = Vector[int]()
    v.push(1)
    v.push(2)
    v.push(3)
    v.push(4)
    print("{v.len()}")
    print("{v.get(0)}")
    int last = v.pop()
    print("{last}")
    print("{v.len()}")
    v.set(0, 99)
    print("{v.get(0)}")
    int removed = v.remove(1)
    print("{removed}")
    print("{v.len()}")
    v.clear()
    print("{v.len()}")
    if v.is_empty():
        print("empty")
