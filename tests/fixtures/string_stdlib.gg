void main():
    str s = "Hello, World!"

    # starts_with / ends_with via interpolation
    print("{s.starts_with("Hello")}")
    print("{s.ends_with("World!")}")
    print("{s.starts_with("World")}")

    # is_empty
    str empty = ""
    print("{empty.is_empty()}")
    print("{s.is_empty()}")

    # contains
    print("{s.contains("World")}")

    # trim â€” print method call result directly
    str padded = "  hi  "
    print(padded.trim())

    # to_upper / to_lower
    print(s.to_upper())
    print(s.to_lower())

    # replace
    str replaced = s.replace("World", "Gorget")
    print(replaced)

    # split
    str csv = "a,b,c"
    Vector[str] parts = csv.split(",")
    print("{parts.len()}")
