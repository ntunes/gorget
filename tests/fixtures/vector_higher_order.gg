from std.collections import Vector

void main():
    Vector[int] v = Vector[int]()
    v.push(1)
    v.push(2)
    v.push(3)
    v.push(4)
    v.push(5)

    # filter: keep even numbers
    Vector[int] evens = v.filter((int x): x % 2 == 0)
    print("{evens.len()}")
    print("{evens.get(0)}")
    print("{evens.get(1)}")

    # map: double each element
    Vector[int] doubled = v.map((int x): x * 2)
    print("{doubled.len()}")
    print("{doubled.get(0)}")
    print("{doubled.get(4)}")

    # fold: sum all elements
    int total = v.fold(0, (int acc, int x): acc + x)
    print("{total}")

    # reduce: sum via reduce
    int reduced = v.reduce((int a, int b): a + b)
    print("{reduced}")
