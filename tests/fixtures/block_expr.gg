void main():
    # block as initializer
    int x = do:
        int a = 5
        int b = 10
        a + b

    print("{x}")

    # simple do block
    int y = do:
        int n = 3
        n * n

    print("{y}")

    # do block with multiple steps
    int z = do:
        int first = 10
        int second = first + 5
        second * 2

    print("{z}")

    # do block with if as tail
    int w = do:
        int n = 10
        if n > 5:
            n + 1
        else:
            n - 1

    print("{w}")

    # do block with match as tail
    int m = do:
        int val = 2
        match val:
            case 1:
                10
            case 2:
                20
            else:
                0

    print("{m}")
