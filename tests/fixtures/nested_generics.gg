from std.collections import Vector, Dict

void main():
    # Vector[Vector[int]] — nested collections
    Vector[Vector[int]] grid = Vector[Vector[int]]()
    Vector[int] row1 = Vector[int]()
    row1.push(1)
    row1.push(2)
    Vector[int] row2 = Vector[int]()
    row2.push(3)
    row2.push(4)
    grid.push(row1)
    grid.push(row2)
    print("{grid.len()}")
    Vector[int] first_row = grid.get(0)
    print("{first_row.get(0)}")
    print("{first_row.get(1)}")
    Vector[int] second_row = grid.get(1)
    print("{second_row.get(0)}")
    print("{second_row.get(1)}")

    # Option[Vector[int]] — Option wrapping a collection
    Vector[int] nums = Vector[int]()
    nums.push(10)
    nums.push(20)
    Option[Vector[int]] some_vec = Some(nums)
    Vector[int] unwrapped = some_vec.unwrap()
    print("{unwrapped.get(0)}")
    print("{unwrapped.len()}")

    # Dict[str, Vector[int]] — Dict with collection values
    Dict[str, Vector[int]] groups = Dict[str, Vector[int]]()
    Vector[int] evens = Vector[int]()
    evens.push(2)
    evens.push(4)
    groups.put("evens", evens)
    Vector[int] odds = Vector[int]()
    odds.push(1)
    odds.push(3)
    groups.put("odds", odds)
    print("{groups.len()}")
    Vector[int] got_evens = groups.get("evens")
    print("{got_evens.get(0)}")
    print("{got_evens.get(1)}")
