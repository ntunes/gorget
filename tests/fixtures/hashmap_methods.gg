from std.collections import Dict

void main():
    Dict[int, int] m = Dict[int, int]()
    m.put(1, 10)
    m.put(2, 20)
    m.put(3, 30)
    int len = m.len()
    print("{len}")
    int val = m.get(2)
    print("{val}")
    if m.contains(1):
        print("has 1")
    if m.contains(99) == false:
        print("no 99")
    m.remove(2)
    print("{m.len()}")
    if m.contains(2) == false:
        print("removed 2")
    m.clear()
    print("{m.len()}")
    if m.is_empty():
        print("empty")
