from std.collections import Set

void main():
    Set[int] a = Set[int]()
    a.add(1)
    a.add(2)
    a.add(3)
    a.add(4)

    Set[int] b = Set[int]()
    b.add(3)
    b.add(4)
    b.add(5)
    b.add(6)

    # union: {1,2,3,4,5,6}
    Set[int] u = a.union(b)
    print("{u.len()}")

    # intersection: {3,4}
    Set[int] inter = a.intersection(b)
    print("{inter.len()}")
    if inter.contains(3):
        print("has 3")
    if inter.contains(4):
        print("has 4")

    # difference: {1,2}
    Set[int] d = a.difference(b)
    print("{d.len()}")
    if d.contains(1):
        print("has 1")
    if d.contains(2):
        print("has 2")
