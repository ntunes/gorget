from std.collections import Vector

void main():
    str s = "hello world"

    # substring
    print(s.substring(0, 5))
    print(s.substring(6, 11))

    # char_at
    char c = s.char_at(0)
    print("{c}")

    # index_of
    print("{s.index_of("world")}")
    print("{s.index_of("xyz")}")

    # count
    str s2 = "abcabcabc"
    print("{s2.count("abc")}")
    print("{s2.count("x")}")

    # repeat
    print("ha".repeat(3))
    print("!".repeat(0))

    # join
    Vector[str] parts = Vector[str]()
    parts.push("a")
    parts.push("b")
    parts.push("c")
    print(", ".join(parts))
    print("-".join(parts))
